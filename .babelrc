{
  "presets": [
    ["@babel/env", {
      "modules": false,
      "loose": true,
      "targets": {
        "browsers": ["last 1 version"]
      }
    }],
    "@babel/preset-flow",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-object-rest-spread"
  ]
}